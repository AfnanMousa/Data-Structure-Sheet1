public class fabonacci {
	static  double  num;
	 public static double seq(int x)
		{
			
			if(x==1)
			{
				return 1;
			}
			else if(x==0)
			{
				return 0;
			}
			else 
			{
				double y=0,z=1;
				for(int i=2;i<=x;i++)
				{
				   num =z+y;
					y=z;
					z=num;
					//System.out.println(num);
				 }
				return num;
			}
			
			
		}
}
